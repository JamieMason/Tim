extends layout

block content
  h1= title

  if users
    table
      thead
        tr
          th(scope='col')= lang('LABEL_NAME')
          th(scope='col')= lang('LABEL_EMAIL')
          th(scope='col')= lang('LABEL_JOIN_DATE')
      tbody
      each user in users
        tr
          td= user.name
          td= user.email
          td= user.created
